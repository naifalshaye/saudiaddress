{
    "name": "naif/saudiaddress",
    "description": "Laravel wrapper for the Saudi National Address APIs",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "php",
        "saudi",
        "saudiaddress",
        "Saudi Arabia",
        "address",
        "addresses",
        "national-address"
    ],
    "authors": [
        {
            "name": "Naif Alshaye",
            "email": "naif@naif.io"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5|^4.0|^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Naif\\Saudiaddress\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Naif\\Saudiaddress\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naif\\Saudiaddress\\SaudiAddressServiceProvider"
            ],
            "aliases": {
                "SaudiAddress": "Naif\\Saudiaddress\\Facades\\SaudiAddress"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
